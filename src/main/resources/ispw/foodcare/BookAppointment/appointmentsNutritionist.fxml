<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="15" style="-fx-padding: 20;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ispw.foodcare.controller.viewcontroller.AppointmentsNutritionistGuiController">
   <HBox prefHeight="17.0" prefWidth="700.0">
      <children>
      
          <Label alignment="CENTER" prefHeight="27.0" prefWidth="699.0" style="-fx-font-size: 18px; -fx-font-weight: bold;" text="Appuntamenti Prenotati" />
      </children>
   </HBox>

    <TableView fx:id="appointmentsTableView" prefHeight="400" prefWidth="700">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="150" text="Nome" />
            <TableColumn fx:id="surnameColumn" prefWidth="150" text="Cognome" />
            <TableColumn fx:id="dateColumn" prefWidth="150" text="Data" />
            <TableColumn fx:id="timeColumn" prefWidth="100" text="Ora" />
            <TableColumn fx:id="notesColumn" prefWidth="300" text="Note" />
        </columns>
    </TableView>
</VBox>
