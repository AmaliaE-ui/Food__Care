<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ispw.foodcare.controller.viewcontroller.PersonalAreaNutritionistGuiController"
      spacing="10.0">

    <padding>
        <Insets top="20.0" right="20.0" bottom="20.0" left="20.0"/>
    </padding>

    <Label text="Area Personale - Nutrizionista"
           style="-fx-font-size: 20px; -fx-font-weight: bold;"/>

    <Label fx:id="nameLabel" text="Nome:"/>
    <Label fx:id="emailLabel" text="Email:"/>
    <Label fx:id="phoneLabel" text="Telefono:"/>
    <Label fx:id="specializationLabel" text="Specializzazione:"/>
</VBox>
